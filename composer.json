{
    "name": "rabbaz/core",
    "description": "Do Rabbaz in your house.",
    "keywords": ["music"],
    "homepage": "https://github.com/opi99/rabbaz",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Alexander Opitz",
            "role": "Developer",
            "homepage": "https://github.com/opi99"
        }
    ],
    "support": {
        "general": ""
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": "^8.2",
        "fortawesome/font-awesome": "^6.3",
        "forward/forwardfw": "@dev",
        "kodus/chrome-logger": "^1.2",
        "psr/event-dispatcher": "^1.0",
        "twbs/bootstrap": "^5.2",
        "twig/twig": "^3.0"
    },
    "suggest": {
    },
    "replace": {
        "rabbaz/core": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Rabbaz\\Core\\": "src/Backend/Core",
            "Rabbaz\\Server\\": "src/Server"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    }
}
